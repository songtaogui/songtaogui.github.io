@article{2020-Cheng-Comparison-BB,
  title = {Comparison and Integration of Computational Methods for Deleterious Synonymous Mutation Prediction},
  author = {Cheng, Na and Li, Menglu and Zhao, Le and Zhang, Bo and Yang, Yuhua and Zheng, Chun-Hou and Xia, Junfeng},
