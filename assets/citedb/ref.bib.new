	issn = {22111247},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S2211124712003427},
	doi = {10.1016/j.celrep.2012.10.006},
	abstract = {Expansions of simple DNA repeats cause numerous hereditary diseases in humans. We analyzed the role of DNA polymerases in the instability of Friedreich’s ataxia (GAA)n repeats in a yeast experimental system. The elementary step of expansion corresponded to \$160 bp in the wild-type strain, matching the size of Okazaki fragments in yeast. This step increased when DNA polymerase a was mutated, suggesting a link between the scale of expansions and Okazaki fragment size. Expandable repeats strongly elevated the rate of mutations at substantial distances around them, a phenomenon we call repeat-induced mutagenesis (RIM). Notably, defects in the replicative DNA polymerases d and ε strongly increased rates for both repeat expansions and RIM. The increases in repeat-mediated instability observed in DNA polymerase d mutants depended on translesion DNA polymerases. We conclude that repeat expansions and RIM are two sides of the same replicative mechanism.},
	language = {en},
	number = {5},
	urldate = {2022-05-22},
	journal = {Cell Reports},
	author = {Shah, Kartik A. and Shishkin, Alexander A. and Voineagu, Irina and Pavlov, Youri I. and Shcherbakova, Polina V. and Mirkin, Sergei M.},
	month = nov,
	year = {2012},
	keywords = {dsb},
	pages = {1088--1095},
	file = {2012_Cell_Reports_Shah_Role_of_DNA_Polymerases_in_Repeat-Mediated_Genome_Instability.pdf:D\:\\Works\\zotfile\\Cell Reports\\2012_Cell_Reports_Shah_Role_of_DNA_Polymerases_in_Repeat-Mediated_Genome_Instability.pdf:application/pdf},
}

