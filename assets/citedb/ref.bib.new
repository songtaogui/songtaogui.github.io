	issn = {1932-6203},
	url = {https://dx.plos.org/10.1371/journal.pone.0050961},
	doi = {10.1371/journal.pone.0050961},
	abstract = {The characterization of biomolecules from ancient samples can shed otherwise unobtainable insights into the past. Despite the fundamental role of transcriptomal change in evolution, the potential of ancient RNA remains unexploited – perhaps due to dogma associated with the fragility of RNA. We hypothesize that seeds offer a plausible refuge for long-term RNA survival, due to the fundamental role of RNA during seed germination. Using RNA-Seq on cDNA synthesized from nucleic acid extracts, we validate this hypothesis through demonstration of partial transcriptomal recovery from two sources of ancient maize kernels. The results suggest that ancient seed transcriptomics may offer a powerful new tool with which to study plant domestication.},
	language = {en},
	number = {1},
	urldate = {2022-10-07},
	journal = {PLoS ONE},
	author = {Fordyce, Sarah L. and Ávila-Arcos, Maria C. and Rasmussen, Morten and Cappellini, Enrico and Romero-Navarro, J. Alberto and Wales, Nathan and Alquezar-Planas, David E. and Penfield, Steven and Brown, Terence A. and Vielle-Calzada, Jean-Philippe and Montiel, Rafael and Jørgensen, Tina and Odegaard, Nancy and Jacobs, Michael and Arriaza, Bernardo and Higham, Thomas F. G. and Ramsey, Christopher Bronk and Willerslev, Eske and Gilbert, M. Thomas P.},
	editor = {Fuller, Dorian Q.},
	month = jan,
	year = {2013},
	pages = {e50961},
	file = {2013_PLoS_ONE_Fordyce_Deep_Sequencing_of_RNA_from_Ancient_Maize_Kernels.pdf:D\:\\Works\\zotfile\\PLoS ONE\\2013_PLoS_ONE_Fordyce_Deep_Sequencing_of_RNA_from_Ancient_Maize_Kernels.pdf:application/pdf},
