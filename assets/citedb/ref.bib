@article{2021-Oh-Machine-BB,
  title = {Machine Learning-Based Analysis of Multi-Omics Data on the Cloud for Investigating Gene Regulations},
  author = {Oh, Minsik and Park, Sungjoon and Kim, Sun and Chae, Heejoon},
  year = {2021},
