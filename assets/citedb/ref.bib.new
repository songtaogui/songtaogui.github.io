	author = {Sun, Chen and Hu, Zhiqiang and Zheng, Tianqing and Lu, Kuangchen and Zhao, Yue and Wang, Wensheng and Shi, Jianxin and Wang, Chunchao and Lu, Jinyuan and Zhang, Dabing and Li, Zhikang and Wei, Chaochun},
	month = jan,
	year = {2017},
	keywords = {pangenomics},
	pages = {597--605},
	file = {2017_Nucleic_Acids_Research_Sun_RPAN.pdf:D\:\\Works\\zotfile\\Nucleic Acids Research\\2017_Nucleic_Acids_Research_Sun_RPAN.pdf:application/pdf},
}

