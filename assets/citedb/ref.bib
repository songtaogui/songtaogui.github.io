@article{2018-Miller-Highly-GG,
  title = {Highly {{Contiguous Genome Assemblies}} of 15 {{{\emph{Drosophila}}}} {{Species Generated Using Nanopore Sequencing}}},
  author = {Miller, Danny E and Staber, Cynthia and Zeitlinger, Julia and Hawley, R Scott},
