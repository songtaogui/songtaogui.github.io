	keywords = {snp, variations},
	pages = {512--515},
	file = {2017_Science_Swarts_Genomic_estimation_of_complex_traits_reveals_ancient_maize_adaptation_to.pdf:D\:\\Works\\zotfile\\Science\\2017_Science_Swarts_Genomic_estimation_of_complex_traits_reveals_ancient_maize_adaptation_to3.pdf:application/pdf;2017_Science_Swarts_Genomic_estimation_of_complex_traits_reveals_ancient_maize_adaptation_to.pdf:D\:\\Works\\zotfile\\Science\\2017_Science_Swarts_Genomic_estimation_of_complex_traits_reveals_ancient_maize_adaptation_to.pdf:application/pdf;aam9425_table_s1.xlsx:D\:\\Works\\zotero\\storage\\44QAVA5Y\\aam9425_table_s1.xlsx:application/vnd.openxmlformats-officedocument.spreadsheetml.sheet},
