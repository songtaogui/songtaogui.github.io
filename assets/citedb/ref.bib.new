	number = {1},
	urldate = {2022-05-22},
	journal = {Nature Communications},
	author = {Jakubosky, David and D’Antonio, Matteo and Bonder, Marc Jan and Smail, Craig and Donovan, Margaret K. R. and Young Greenwald, William W. and Matsui, Hiroko and {i2QTL Consortium} and Bonder, Marc J. and Cai, Na and Carcamo-Orive, Ivan and D’Antonio, Matteo and Frazer, Kelly A. and Young Greenwald, William W. and Jakubosky, David and Knowles, Joshua W. and Matsui, Hiroko and McCarthy, Davis J. and Mirauta, Bogdan A. and Montgomery, Stephen B. and Quertermous, Thomas and Seaton, Daniel D. and Smail, Craig and Smith, Erin N. and Stegle, Oliver and D’Antonio-Chronowska, Agnieszka and Stegle, Oliver and Smith, Erin N. and Montgomery, Stephen B. and DeBoever, Christopher and Frazer, Kelly A.},
