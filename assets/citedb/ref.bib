@article{2021-Babarinde-Transposable-NAR,
  title = {Transposable Element Sequence Fragments Incorporated into Coding and Noncoding Transcripts Modulate the Transcriptome of Human Pluripotent Stem Cells},
  author = {Babarinde, Isaac~A and Ma, Gang and Li, Yuhao and Deng, Boping and Luo, Zhiwei and Liu, Hao and Abdul, Mazid~Md and Ward, Carl and Chen, Minchun and Fu, Xiuling and Shi, Liyang and Duttlinger, Martha and He, Jiangping and Sun, Li and Li, Wenjuan and Zhuang, Qiang and Tong, Guoqing and Frampton, Jon and Cazier, Jean-Baptiste and Chen, Jiekai and Jauch, Ralf and Esteban, Miguel~A and Hutchins, Andrew~P},
  year = {2021},
