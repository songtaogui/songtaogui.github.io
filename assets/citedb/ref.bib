@article{2016-Morandin-Comparative-GB,
  title = {Comparative Transcriptomics Reveals the Conserved Building Blocks Involved in Parallel Evolution of Diverse Phenotypic Traits in Ants},
  author = {Morandin, Claire and Tin, Mandy M. Y. and Abril, S{\'i}lvia and G{\'o}mez, Crisanto and Pontieri, Luigi and Schi{\o}tt, Morten and Sundstr{\"o}m, Liselotte and Tsuji, Kazuki and Pedersen, Jes S{\o}e and Helanter{\"a}, Heikki and Mikheyev, Alexander S.},
