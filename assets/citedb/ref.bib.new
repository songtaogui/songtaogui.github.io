	journal = {Nature Genetics},
	author = {{23andMe Research Team} and {Social Science Genetic Association Consortium} and Turley, Patrick and Walters, Raymond K. and Maghzian, Omeed and Okbay, Aysu and Lee, James J. and Fontana, Mark Alan and Nguyen-Viet, Tuan Anh and Wedow, Robbee and Zacher, Meghan and Furlotte, Nicholas A. and Magnusson, Patrik and Oskarsson, Sven and Johannesson, Magnus and Visscher, Peter M. and Laibson, David and Cesarini, David and Neale, Benjamin M. and Benjamin, Daniel J.},
	month = feb,
