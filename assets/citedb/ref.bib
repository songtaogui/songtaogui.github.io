@article{2009-Liu-Comparative-GR,
  title = {Comparative Analysis of {{{\emph{Alu}}}} Repeats in Primate Genomes},
  author = {Liu, George E. and Alkan, Can and Jiang, Lu and Zhao, Shaying and Eichler, Evan E.},
  year = {2009},
  month = may,
  journal = {Genome Research},
  volume = {19},
