@article{2020-Qiu-Diverse-GB,
  title = {Diverse Genetic Mechanisms Underlie Worldwide Convergent Rice Feralization},
  author = {Qiu, Jie and Jia, Lei and Wu, Dongya and Weng, Xifang and Chen, Lijuan and Sun, Jian and Chen, Meihong and Mao, Lingfeng and Jiang, Bowen and Ye, Chuyu and Turra, Guilherme Menegol and Guo, Longbiao and Ye, Guoyou and Zhu, Qian-Hao and Imaizumi, Toshiyuki and Song, Beng-Kah and Scarabel, Laura and Merotto, Aldo and Olsen, Kenneth M. and Fan, Longjiang},
