}

@article{forsdykeNeutralismSelectionismChargaff2021,
	title = {Neutralism versus selectionism: {Chargaff}'s second parity rule, revisited},
	volume = {149},
	issn = {0016-6707, 1573-6857},
	shorttitle = {Neutralism versus selectionism},
	url = {https://link.springer.com/10.1007/s10709-021-00119-5},
	doi = {10.1007/s10709-021-00119-5},
	abstract = {Of Chargaff’s four "rules" on DNA base frequencies, the functional interpretation of his second parity rule (PR2) is the most contentious. Thermophile base compositions (GC\%) were taken by Galtier and Lobry (1997) as favoring Sueoka’s neutral PR2 hypothesis over Forsdyke’s selective PR2 hypothesis, namely that mutations improving local within-species recombination efficiency had generated a genome-wide potential for the strands of duplex DNA to separate and initiate recombination through the "kissing" of the tips of stem-loops. However, following Chargaff’s GC rule, base composition mainly reflects a species-specific, genome-wide, evolutionary pressure. GC\% could not have consistently followed the dictates of temperature, since it plays fundamental roles in both sustaining species integrity and, through primarily neutral genome-wide mutation, fostering speciation. Evidence for a local within-species recombination-initiating role of base order was obtained with a novel technology that masked the contribution of base composition to nucleic acid folding energy. Forsdyke’s results were consistent with his PR2 hypothesis, appeared to resolve some root problems in biology and provided a theoretical underpinning for alignment-free taxonomic analyses using relative oligonucleotide frequencies (k-mer analysis). Moreover, consistent with Chargaff’s cluster rule, discovery of the thermoadaptive role of the "purine-loading" of open reading frames made less tenable the Galtier-Lobry anti-selectionist arguments.},
	language = {en},
	number = {2},
	urldate = {2022-06-13},
	journal = {Genetica},
	author = {Forsdyke, Donald R.},
	month = apr,
	year = {2021},
	pages = {81--88},
	file = {2021_Genetica_Forsdyke_Neutralism_versus_selectionism.pdf:D\:\\Works\\zotfile\\Genetica\\2021_Genetica_Forsdyke_Neutralism_versus_selectionism.pdf:application/pdf},
}

