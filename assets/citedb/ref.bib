@article{2016-Wang-Power-MP,
  title = {The {{Power}} of {{Inbreeding}}: {{NGS-Based GWAS}} of {{Rice Reveals Convergent Evolution}} during {{Rice Domestication}}},
  shorttitle = {The {{Power}} of {{Inbreeding}}},
  author = {Wang, Hongru and Xu, Xun and Vieira, Filipe~Garrett and Xiao, Yunhua and Li, Zhikang and Wang, Jun and Nielsen, Rasmus and Chu, Chengcai},
  year = {2016},
