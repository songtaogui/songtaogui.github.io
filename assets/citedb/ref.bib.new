@article{deleeuwDiagnosticsShortTandem2019,
	title = {Diagnostics of short tandem repeat expansion variants using massively parallel sequencing and componential tools},
	volume = {27},
	issn = {1018-4813, 1476-5438},
	url = {http://www.nature.com/articles/s41431-018-0302-4},
	doi = {10.1038/s41431-018-0302-4},
	abstract = {Short tandem repeats (STRs) are scattered throughout the human genome. Some STRs, like trinucleotide repeat expansion (TRE) variants, cause hereditable disorders. Unambiguous molecular diagnostics of TRE disorders is hampered by current technical limitations imposed by traditional PCR and DNA sequencing methods. Here we report a novel pipeline for TRE variant diagnosis employing the massively parallel sequencing (MPS) combined with an opensource software package (FDSTools), which together are designed to distinguish true STR sequences from STR sequencing artifacts. We show that this approach can improve TRE diagnosis, such as Oculopharyngeal muscular dystrophy (OPMD). OPMD is caused by a trinucleotide expansion in the PABPN1 gene. A short GCN expansion, (GCN[10]), coding for a 10 alanine repeat is not pathogenic, but an alanine expansion is pathogenic. Applying this novel procedure in a Dutch OPMD patient cohort, we found expansion variants from GCN[11] to GCN[16], with the GCN[16] as the most abundant variant. The repeat expansion length did not correlate with clinical features. However, symptom severity was found to correlate with age and with the initial affected muscles, suggesting that aging and muscle-speciﬁc factors can play a role in modulating OPMD.},
	language = {en},
	number = {3},
	urldate = {2022-05-22},
	journal = {European Journal of Human Genetics},
	author = {de Leeuw, Rick H. and Garnier, Dominique and Kroon, Rosemarie M. J. M. and Horlings, Corinne G. C. and de Meijer, Emile and Buermans, Henk and van Engelen, Baziel G. M. and de Knijff, Peter and Raz, Vered},
	month = mar,
	year = {2019},
	keywords = {str},
	pages = {400--407},
	file = {2019_European_Journal_of_Human_Genetics_de_Leeuw_Diagnostics_of_short_tandem_repeat_expansion_variants_using_massively_parallel.pdf:D\:\\Works\\zotfile\\European Journal of Human Genetics\\2019_European_Journal_of_Human_Genetics_de_Leeuw_Diagnostics_of_short_tandem_repeat_expansion_variants_using_massively_parallel.pdf:application/pdf},
