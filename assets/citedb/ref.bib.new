}

@article{whitlockReliableDetectionLoci2015,
	title = {Reliable {Detection} of {Loci} {Responsible} for {Local} {Adaptation}: {Inference} of a {Null} {Model} through {Trimming} the {Distribution} of \textit{{F}} $_{\textrm{{ST}}}$},
	volume = {186},
	issn = {0003-0147, 1537-5323},
	shorttitle = {Reliable {Detection} of {Loci} {Responsible} for {Local} {Adaptation}},
	url = {https://www.journals.uchicago.edu/doi/10.1086/682949},
	doi = {10.1086/682949},
	abstract = {Loci responsible for local adaptation are likely to have more genetic differentiation among populations than neutral loci. However, neutral loci can vary widely in their amount of genetic differentiation, even over the same geographic range. Unfortunately, the distribution of differentiation—as measured by an index such as FST — depends on the details of the demographic history of the populations in question, even without spatially heterogeneous selection. Many methods designed to detect FST outliers assume a speciﬁc model of demographic history, which can result in extremely high false positive rates for detecting loci under selection. We develop a new method that infers the distribution of FST for loci unlikely to be strongly affected by spatially diversifying selection, using data on a large set of loci with unknown selective properties. Compared to previous methods, this approach, called OutFLANK, has much lower false positive rates and comparable power, as shown by simulation.},
	language = {en},
	number = {S1},
	urldate = {2022-05-22},
	journal = {The American Naturalist},
	author = {Whitlock, Michael C. and Lotterhos, Katie E.},
	month = oct,
	year = {2015},
	keywords = {evolution},
	pages = {S24--S36},
	file = {2015_The_American_Naturalist_Whitlock_Reliable_Detection_of_Loci_Responsible_for_Local_Adaptation.pdf:D\:\\Works\\zotfile\\The American Naturalist\\2015_The_American_Naturalist_Whitlock_Reliable_Detection_of_Loci_Responsible_for_Local_Adaptation.pdf:application/pdf},
}

