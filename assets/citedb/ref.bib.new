@article{springerMaizeInbredsExhibit2009,
	title = {Maize {Inbreds} {Exhibit} {High} {Levels} of {Copy} {Number} {Variation} ({CNV}) and {Presence}/{Absence} {Variation} ({PAV}) in {Genome} {Content}},
	volume = {5},
