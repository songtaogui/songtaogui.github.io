  institution = {{Bioinformatics}},
  doi = {10.1101/2021.07.06.451246},
  abstract = {Recently, Gagie et al. proposed a version of the FM-index, called the r-index, that can store thousands of human genomes on a commodity computer. Then Kuhnle et al. showed how to build the r-index efficiently via a technique called prefix-free parsing (PFP) and demonstrated its effectiveness for exact pattern matching. Exact pattern matching can be leveraged to support approximate pattern matching but the r-index itself cannot support efficiently popular and important queries such as finding maximal exact matches (MEMs). To address this shortcoming, Bannai et al. introduced the concept of thresholds, and showed that storing them together with the r-index enables efficient MEM finding \textemdash{} but they did not say how to find those thresholds. We present a novel algorithm that applies PFP to build the r-index and find the thresholds simultaneously and in linear time and space with respect to the size of the prefix-free parse. Our implementation called MONI can rapidly find MEMs between reads and large sequence collections of highly repetitive sequences. Compared to other read aligners \textendash PuffAligner, Bowtie2, BWA-MEM, and CHIC \textendash{} MONI used 2\textendash 11 times less memory and was 2\textendash 32 times faster for index construction. Moreover, MONI was less than one thousandth the size of competing indexes for large collections of human chromosomes. Thus, MONI represents a major advance in our ability to perform MEM finding against very large collections of related references.},
  langid = {english},
  keywords = {graphgenomics},
  file = {D\:\\Works\\zotfile\\Bioinformatics2021\\2021_Rossi_MONI.pdf}
}

@article{2021-Rue-Genetic-P,
  title = {Genetic Diversity across the Mitochondrial Genome of Eastern Oysters ( {{{\emph{Crassostrea}}}}{\emph{ Virginica}} ) in the Northern {{Gulf}} of {{Mexico}}},
  author = {Rue, Chani R. and Selwyn, Jason D. and Cockett, Patricia M. and Gillis, Bryan and Gurski, Lauren and Jose, Philip and Kutil, Brandi L. and Magnuson, Sharon F. and {{\'A}ngela L{\'o}pez de Mesa}, Luz and Overath, R Deborah and Smee, Delbert Lee and Bird, Christopher E.},
