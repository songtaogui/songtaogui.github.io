@article{2012-Zhen-Parallel-S,
  title = {Parallel {{Molecular Evolution}} in an {{Herbivore Community}}},
  author = {Zhen, Ying and Aardema, Matthew L. and Medina, Edgar M. and Schumer, Molly and Andolfatto, Peter},
