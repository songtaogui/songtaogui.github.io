@article{ammirajuEvolutionaryDynamicsAncient2007,
	title = {Evolutionary dynamics of an ancient retrotransposon family provides insights into evolution of genome size in the genus {Oryza}: {Evolutionary} dynamics of ancient retrotransposon family},
	volume = {52},
	issn = {09607412, 1365313X},
	shorttitle = {Evolutionary dynamics of an ancient retrotransposon family provides insights into evolution of genome size in the genus {Oryza}},
	url = {https://onlinelibrary.wiley.com/doi/10.1111/j.1365-313X.2007.03242.x},
	doi = {10.1111/j.1365-313X.2007.03242.x},
	abstract = {Long terminal repeat (LTR) retrotransposons constitute a signiﬁcant portion of most eukaryote genomes and can dramatically change genome size and organization. Although LTR retrotransposon content variation is well documented, the dynamics of genomic ﬂux caused by their activity are poorly understood on an evolutionary time scale. This is primarily because of the lack of an experimental system composed of closely related species whose divergence times are within the limits of the ability to detect ancestrally related retrotransposons. The genus Oryza, with 24 species, ten genome types, different ploidy levels and over threefold genome size variation, constitutes an ideal experimental system to explore genus-level transposon dynamics. Here we present data on the discovery and characterization of an LTR retrotransposon family named RWG in the genus Oryza. Comparative analysis of transposon content (approximately 20 to 27 000 copies) and transpositional history of this family across the genus revealed a broad spectrum of independent and lineage-speciﬁc changes that have implications for the evolution of genome size and organization. In particular, we provide evidence that the basal GG genome of Oryza (O. granulata) has expanded by nearly 25\% by a burst of the RWG lineage Gran3 subsequent to speciation. Finally we describe the recent evolutionary origin of Dasheng, a large retrotransposon derivative of the RWG family, speciﬁcally found in the A, B and C genome lineages of Oryza.},
	language = {en},
	number = {2},
	urldate = {2022-07-12},
	journal = {The Plant Journal},
	author = {Ammiraju, Jetty S. S. and Zuccolo, Andrea and Yu, Yeisoo and Song, Xiang and Piegu, Benoit and Chevalier, Frederic and Walling, Jason G. and Ma, Jianxin and Talag, Jayson and Brar, Darshan S. and SanMiguel, Phillip J. and Jiang, Ning and Jackson, Scott A. and Panaud, Olivier and Wing, Rod A.},
	month = aug,
	year = {2007},
	pages = {342--351},
	file = {2007_The_Plant_Journal_Ammiraju_Evolutionary_dynamics_of_an_ancient_retrotransposon_family_provides_insights.pdf:D\:\\Works\\zotfile\\The Plant Journal\\2007_The_Plant_Journal_Ammiraju_Evolutionary_dynamics_of_an_ancient_retrotransposon_family_provides_insights.pdf:application/pdf},
}

@article{wongExpansionSingleTransposable2019,
	title = {Expansion of a single transposable element family is associated with genome-size increase and radiation in the genus \textit{{Hydra}}},
	volume = {116},
	issn = {0027-8424, 1091-6490},
	url = {https://pnas.org/doi/full/10.1073/pnas.1910106116},
	doi = {10.1073/pnas.1910106116},
	abstract = {Transposable elements are one of the major contributors to genome-size differences in metazoans. Despite this, relatively little is known about the evolutionary patterns of element expansions and the element families involved. Here we report a broad genomic sampling within the genus
              Hydra
              , a freshwater cnidarian at the focal point of diverse research in regeneration, symbiosis, biogeography, and aging. We find that the genome of
              Hydra
              is the result of an expansion event involving long interspersed nuclear elements and in particular a single family of the chicken repeat 1 (CR1) class. This expansion is unique to a subgroup of the genus
              Hydra
              , the brown hydras, and is absent in the green hydra, which has a repeat landscape similar to that of other cnidarians. These features of the genome make
              Hydra
              attractive for studies of transposon-driven genome expansions and speciation.},
	language = {en},
	number = {46},
	urldate = {2022-07-12},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Wong, Wai Yee and Simakov, Oleg and Bridge, Diane M. and Cartwright, Paulyn and Bellantuono, Anthony J. and Kuhn, Anne and Holstein, Thomas W. and David, Charles N. and Steele, Robert E. and Martínez, Daniel E.},
	month = nov,
	year = {2019},
	pages = {22915--22917},
	file = {2019_Proceedings_of_the_National_Academy_of_Sciences_Wong_Expansion_of_a_single_transposable_element_family_is_associated_with.pdf:D\:\\Works\\zotfile\\Proceedings of the National Academy of Sciences\\2019_Proceedings_of_the_National_Academy_of_Sciences_Wong_Expansion_of_a_single_transposable_element_family_is_associated_with.pdf:application/pdf},
}

@article{devosGenomeSizeReduction2002,
	title = {Genome {Size} {Reduction} through {Illegitimate} {Recombination} {Counteracts} {Genome} {Expansion} in \textit{{Arabidopsis}}},
	volume = {12},
	issn = {1088-9051, 1549-5469},
	url = {http://genome.cshlp.org/lookup/doi/10.1101/gr.132102},
	doi = {10.1101/gr.132102},
	abstract = {Genome size varies greatly across angiosperms. It is well documented that, in addition to polyploidization, retrotransposon amplification has been a major cause of genome expansion. The lack of evidence for counterbalancing mechanisms that curtail unlimited genome growth has made many of us wonder whether angiosperms have a “one-way ticket to genomic obesity.” We have therefore investigated an angiosperm with a well-characterized and notably small genome,
              Arabidopsis thaliana
              , for evidence of genomic DNA loss. Our results indicate that illegitimate recombination is the driving force behind genome size decrease in
              Arabidopsis
              , removing at least fivefold more DNA than unequal homologous recombination. The presence of highly degraded retroelements also suggests that retrotransposon amplification has not been confined to the last 4 million years, as is indicated by the dating of intact retroelements.},
	language = {en},
	number = {7},
	urldate = {2022-07-12},
	journal = {Genome Research},
	author = {Devos, Katrien M. and Brown, James K.M. and Bennetzen, Jeffrey L.},
	month = jul,
	year = {2002},
	pages = {1075--1079},
	file = {2002_Genome_Research_Devos_Genome_Size_Reduction_through_Illegitimate_Recombination_Counteracts_Genome.pdf:D\:\\Works\\zotfile\\Genome Research\\2002_Genome_Research_Devos_Genome_Size_Reduction_through_Illegitimate_Recombination_Counteracts_Genome.pdf:application/pdf},
}

@article{jayaswalGenomeWideAnnotation2020,
	title = {Genome wide annotation and characterization of young, intact long terminal repeat retrotransposons ({In}-{LTR}-{RTs}) of seven legume species},
	volume = {148},
	issn = {0016-6707, 1573-6857},
	url = {https://link.springer.com/10.1007/s10709-020-00103-5},
	doi = {10.1007/s10709-020-00103-5},
	abstract = {Availability of genome sequence of different legume species has provided an opportunity to characterize the abundance, distribution, and divergence of canonical intact long terminal retrotransposons (In-LTR-RT) superfamilies. Among seven legume species, Arachis ipaensis (Aip) showed the highest number of full-length canonical In-LTR-RTs (3325), followed by Glycine max (Gma, 2328), Vigna angularis (Van, 1625), Arachis durensis (Adu, 1348), Lotus japonicus (Lja, 1294), Medicago truncatula (Mtr, 788), and Circer arietinum (Car, 124). Divergence time analysis demonstrated that the amplification timeframe of LTR-RTs dramatically varied in different families. The average insertion time of Copia element varied from 0.51 (Van) to 1.37 million years ago (Mya) (Adu, and Aip), whereas that of Gypsy was between 0.22 (Mtr) and 1.82 Mya (Adu). Bayesian phylogenetic tree analysis suggested that the 1397 and 1917 reverse transcriptase (RT) domains of Copia and Gypsy families of the seven legume species were clustered into 7 and 14 major groups, respectively. The highest proportion (approximately 94.79–100\%) of transposable element (TE)-associated genes assigned to pathways was mapped to metabolism-related pathways in all species. The results enabled the structural understanding of full-length In-LTR-RTs and will be valuable resource for the further study of the impact of TEs on gene structure and expression in legume species.},
	language = {en},
	number = {5-6},
	urldate = {2022-07-12},
	journal = {Genetica},
	author = {Jayaswal, Pawan Kumar and Shanker, Asheesh and Singh, Nagendra Kumar},
	month = dec,
	year = {2020},
	pages = {253--268},
	file = {2020_Genetica_Jayaswal_Genome_wide_annotation_and_characterization_of_young,_intact_long_terminal.pdf:D\:\\Works\\zotfile\\Genetica\\2020_Genetica_Jayaswal_Genome_wide_annotation_and_characterization_of_young,_intact_long_terminal.pdf:application/pdf},
