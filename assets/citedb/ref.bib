@article{2018-Press-Massive-GR,
  title = {Massive Variation of Short Tandem Repeats with Functional Consequences across Strains of {{{\emph{Arabidopsis}}}}{\emph{ Thaliana}}},
  author = {Press, Maximilian O. and McCoy, Rajiv C. and Hall, Ashley N. and Akey, Joshua M. and Queitsch, Christine},
