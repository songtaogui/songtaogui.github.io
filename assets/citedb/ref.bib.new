@article{layerLUMPYProbabilisticFramework2014,
	title = {{LUMPY}: a probabilistic framework for structural variant discovery},
	volume = {15},
	issn = {1465-6906},
	shorttitle = {{LUMPY}},
	url = {http://genomebiology.biomedcentral.com/articles/10.1186/gb-2014-15-6-r84},
	doi = {10.1186/gb-2014-15-6-r84},
	abstract = {Comprehensive discovery of structural variation (SV) from whole genome sequencing data requires multiple detection signals including read-pair, split-read, read-depth and prior knowledge. Owing to technical challenges, extant SV discovery algorithms either use one signal in isolation, or at best use two sequentially. We present LUMPY, a novel SV discovery framework that naturally integrates multiple SV signals jointly across multiple samples. We show that LUMPY yields improved sensitivity, especially when SV signal is reduced owing to either low coverage data or low intra-sample variant allele frequency. We also report a set of 4,564 validated breakpoints from the NA12878 human genome. https://github.com/arq5x/lumpy-sv.},
	language = {en},
	number = {6},
	urldate = {2022-05-22},
	journal = {Genome Biology},
	author = {Layer, Ryan M and Chiang, Colby and Quinlan, Aaron R and Hall, Ira M},
	year = {2014},
	keywords = {methods},
	pages = {R84},
	file = {2014_Genome_Biology_Layer_LUMPY.pdf:D\:\\Works\\zotfile\\Genome Biology\\2014_Genome_Biology_Layer_LUMPY.pdf:application/pdf},
}

@article{ashkenazyIndelReliabilityIndelBased2014,
	title = {Indel {Reliability} in {Indel}-{Based} {Phylogenetic} {Inference}},
	volume = {6},
	issn = {1759-6653},
	url = {https://academic.oup.com/gbe/article-lookup/doi/10.1093/gbe/evu252},
	doi = {10.1093/gbe/evu252},
	abstract = {It is often assumed that it is unlikely that the same insertion or deletion (indel) event occurred at the same position in two independent evolutionary lineages, and thus, indel-based inference of phylogeny should be less subject to homoplasy compared with standard inference which is based on substitution events. Indeed, indels were successfully used to solve debated evolutionary relationships among various taxonomical groups. However, indels are never directly observed but rather inferred from the alignment and thus indel-based inference may be sensitive to alignment errors. It is hypothesized that phylogenetic reconstruction would be more accurate if it relied only on a subset of reliable indels instead of the entire indel data. Here, we developed a method to quantify the reliability of indel characters by measuring how often they appear in a set of alternative multiple sequence alignments. Our approach is based on the assumption that indels that are consistently present in most alternative alignments are more reliable compared with indels that appear only in a small subset of these alignments. Using simulated and empirical data, we studied the impact of ﬁltering and weighting indels by their reliability scores on the accuracy of indel-based phylogenetic reconstruction. The new method is available as a web-server at http://guidance.tau.ac.il/RELINDEL/.},
	language = {en},
	number = {12},
	urldate = {2022-05-22},
	journal = {Genome Biology and Evolution},
	author = {Ashkenazy, Haim and Cohen, Ofir and Pupko, Tal and Huchon, Dorothée},
	month = dec,
	year = {2014},
	keywords = {methods},
	pages = {3199--3209},
	file = {2014_Genome_Biology_and_Evolution_Ashkenazy_Indel_Reliability_in_Indel-Based_Phylogenetic_Inference.pdf:D\:\\Works\\zotfile\\Genome Biology and Evolution\\2014_Genome_Biology_and_Evolution_Ashkenazy_Indel_Reliability_in_Indel-Based_Phylogenetic_Inference.pdf:application/pdf},
}

