@article{kistlerAncientPlantGenomics2020,
	title = {Ancient {Plant} {Genomics} in {Archaeology}, {Herbaria}, and the {Environment}},
	volume = {71},
	issn = {1543-5008, 1545-2123},
	url = {https://www.annualreviews.org/doi/10.1146/annurev-arplant-081519-035837},
	doi = {10.1146/annurev-arplant-081519-035837},
	abstract = {The ancient DNA revolution of the past 35 years has driven an explosion in the breadth, nuance, and diversity of questions that are approachable using ancient biomolecules, and plant research has been a constant, indispensable facet of these developments. Using archaeological, paleontological, and herbarium plant tissues, researchers have probed plant domestication and dispersal, plant evolution and ecology, paleoenvironmental composition and dynamics, and other topics across related disciplines. Here, we review the development of the ancient DNA discipline and the role of plant research in its progress and refinement. We summarize our understanding of longterm plant DNA preservation and the characteristics of degraded DNA. In addition, we discuss challenges in ancient DNA recovery and analysis and the laboratory and bioinformatic strategies used to mitigate them. Finally, we review recent applications of ancient plant genomic research.},
	language = {en},
	number = {1},
	urldate = {2022-10-07},
	journal = {Annual Review of Plant Biology},
	author = {Kistler, Logan and Bieker, Vanessa C. and Martin, Michael D. and Pedersen, Mikkel Winther and Ramos Madrigal, Jazmín and Wales, Nathan},
	month = apr,
	year = {2020},
	pages = {605--629},
	file = {2020_Annual_Review_of_Plant_Biology_Kistler_Ancient_Plant_Genomics_in_Archaeology,_Herbaria,_and_the_Environment.pdf:D\:\\Works\\zotfile\\Annual Review of Plant Biology\\2020_Annual_Review_of_Plant_Biology_Kistler_Ancient_Plant_Genomics_in_Archaeology,_Herbaria,_and_the_Environment.pdf:application/pdf},
}

@techreport{mckainAncestryTwoSubgenomes2018,
	type = {preprint},
	title = {Ancestry of the two subgenomes of maize},
	url = {http://biorxiv.org/lookup/doi/10.1101/352351},
	abstract = {Maize (Zea mays ssp. mays) is not only one of the world's most important crops, but it also is a powerful tool for studies of genetics, genomics, and cytology. The genome of maize shows the unmistakable signature of an ancient hybridization event followed by whole genome duplication (allopolyploidy), but the parents of this event have been a mystery for over a century, since studies of maize cytogenetics began. Here we show that the whole genome duplication event preceded the divergence of the entire genus Zea and its sister genus Tripsacum. One genome was donated, in whole or in part, by a plant related to the modern African genera Urelytrum and Vossia, although genomic rearrangement has been extensive. The other genome donor is less well-supported, but may have been related to the modern Rottboellia-Hemarthria clade, which is also African. Thus Zea and Tripsacum together represent a New World radiation derived from African ancestors.},
	language = {en},
	urldate = {2022-10-07},
	institution = {Plant Biology},
	author = {McKain, Michael R. and Estep, Matt C. and Pasquet, Rémy and Layton, Daniel J. and Vela Díaz, Dilys M. and Zhong, Jinshun and Hodge, John G. and Malcomber, Simon T. and Chipabika, Gilson and Pallangyo, Beatrice and Kellogg, Elizabeth A.},
	month = jun,
	year = {2018},
	doi = {10.1101/352351},
	file = {2018_McKain_Ancestry_of_the_two_subgenomes_of_maize.pdf:D\:\\Works\\zotfile\\Plant Biology\\2018_McKain_Ancestry_of_the_two_subgenomes_of_maize.pdf:application/pdf},
}

@article{fordyceDeepSequencingRNA2013,
