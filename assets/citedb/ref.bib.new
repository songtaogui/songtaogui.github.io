@article{pandeyEvolutionUnilateralIncompatibility1981,
	title = {Evolution of {Unilateral} {Incompatibility} in {Flowering} {Plants}: {Further} {Evidence} in {Favour} of {Twin} {Specificities} {Controlling} {Intra}- and {Interspecific} {Incompatibility}},
	volume = {89},
	url = {http://www.jstor.org/stable/2431996},
	doi = {10.1111/j.1469-8137.1981.tb02349.x},
	abstract = {The breeding system in Nicotiana glauca is unique in that the species comprises mainly self-compatible (S.C.) green-flowered plants but also includes rare self-incompatible (S.I.) red-flowered races. The S.C. plants of Nicotiana glauca show unilateral interspecificincompatibility with other S.I. species of Nicotiana, a widely occurring characteristic normally associated with S.C. species with relatively long-established self-compatibility. The present study of S.I. and S.C. strains of Nicotiana glauca showed that the \$ S.I. x S.C. S type of unilateral incompatibility (U.I.) occurs within this species, and the U.I. is a function of the S alleles. Of the nine S, alleles studied seven, when present in the homozygous state in the style, made that style reject Sc (Nicotiana glauca) pollen whereas two alleles allowed styles to accept this pollen. Similar polymorphism at the Nicotiana glauca S locus was also found in relation to Sf pollen from the S.C. species Nicotiana langsdorffiiS. , alleles in Nicotiana glauca were classified differentlyfor the polymorphisms in relation to Sc and Sf pollen. The results show that the Sc allele is sporophytically determined causing allelic interaction in the pollen and style. Dominance, co-dominance and competitive interaction were observed. In contrast the Sf allele of Nicotiana langsdorffiis gametophytically determined, stylar alleles always showing independent action.},
	language = {en},
	number = {4},
	journal = {New Phytologist},
	author = {Pandey, K. K.},
	year = {1981},
	pages = {705--728},
	file = {1981_New_Phytologist_Pandey_Evolution_of_Unilateral_Incompatibility_in_Flowering_Plants.pdf:D\:\\Works\\zotfile\\New Phytologist\\1981_New_Phytologist_Pandey_Evolution_of_Unilateral_Incompatibility_in_Flowering_Plants.pdf:application/pdf},
