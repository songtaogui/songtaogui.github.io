@article{uffelmannGenomewideAssociationStudies2021,
	title = {Genome-wide association studies},
	volume = {1},
	issn = {2662-8449},
	url = {https://www.nature.com/articles/s43586-021-00056-9},
	doi = {10.1038/s43586-021-00056-9},
	abstract = {Genome-w ide association studies (GWAS) test hundreds of thousands of genetic variants across many genomes to find those statistically associated with a specific trait or disease. This methodology has generated a myriad of robust associations for a range of traits and diseases, and the number of associated variants is expected to grow steadily as GWAS sample sizes increase. GWAS results have a range of applications, such as gaining insight into a phenotype’s underlying biology, estimating its heritability, calculating genetic correlations, making clinical risk predictions, informing drug development programmes and inferring potential causal relationships between risk factors and health outcomes. In this Primer, we provide the reader with an introduction to GWAS, explaining their statistical basis and how they are conducted, describe state-of-t he art approaches and discuss limitations and challenges, concluding with an overview of the current and future applications for GWAS results.},
	language = {en},
	number = {1},
	urldate = {2022-05-22},
	journal = {Nature Reviews Methods Primers},
	author = {Uffelmann, Emil and Huang, Qin Qin and Munung, Nchangwi Syntia and de Vries, Jantina and Okada, Yukinori and Martin, Alicia R. and Martin, Hilary C. and Lappalainen, Tuuli and Posthuma, Danielle},
	month = dec,
	year = {2021},
	keywords = {gwas, popgenetics},
	pages = {59},
	file = {2021_Nature_Reviews_Methods_Primers_Uffelmann_Genome-wide_association_studies.pdf:D\:\\Works\\zotfile\\Nature Reviews Methods Primers\\2021_Nature_Reviews_Methods_Primers_Uffelmann_Genome-wide_association_studies.pdf:application/pdf;2021_Nature_Reviews_Methods_Primers_Uffelmann_Genome-wide_association_studies.pdf:D\:\\Works\\zotfile\\Nature Reviews Methods Primers\\2021_Nature_Reviews_Methods_Primers_Uffelmann_Genome-wide_association_studies2.pdf:application/pdf;2021_Nature_Reviews_Methods_Primers_Uffelmann_Genome-wide_association_studies.pdf:D\:\\Works\\zotfile\\Nature Reviews Methods Primers\\2021_Nature_Reviews_Methods_Primers_Uffelmann_Genome-wide_association_studies3.pdf:application/pdf},
