@article{2021-Lan-AtMAD-NAR,
  title = {{{AtMAD}}: {{{\emph{Arabidopsis}}}}{\emph{ Thaliana}} Multi-Omics Association Database},
  shorttitle = {{{AtMAD}}},
  author = {Lan, Yiheng and Sun, Ruikun and Ouyang, Jian and Ding, Wubing and Kim, Min-Jun and Wu, Jun and Li, Yuhua and Shi, Tieliu},
  year = {2021},
