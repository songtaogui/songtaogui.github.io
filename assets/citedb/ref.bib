  shorttitle = {{{PPanGGOLiN}}},
  author = {Gautreau, Guillaume and Bazin, Adelme and Gachet, Mathieu and Planel, R{\'e}mi and Burlot, Laura and Dubois, Mathieu and Perrin, Amandine and M{\'e}digue, Claudine and Calteau, Alexandra and Cruveiller, St{\'e}phane and Matias, Catherine and Ambroise, Christophe and Rocha, Eduardo P. C. and Vallenet, David},
