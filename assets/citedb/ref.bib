@techreport{2017-Mejia-Guerra-Mer-,
  type = {Preprint},
  title = {{\emph{K}} -Mer Grammar Uncovers Maize Regulatory Architecture},
  author = {{Mej{\'i}a-Guerra}, Mar{\'i}a Katherine and Buckler, Edward S},
  year = {2017},
  month = nov,
  institution = {{Plant Biology}},
  doi = {10.1101/222927},
  abstract = {Only a small percentage of the genome sequence is involved in regulation of gene expression, but to biochemically identify this portion is expensive and laborious. In species like maize, with diverse intergenic regions and lots of repetitive elements, this is an especially challenging problem. While regulatory regions are rare, they do have characteristic chromatin contexts and sequence organization (the grammar) with which they can be identified. We developed a computational framework to exploit this sequence arrangement. The models learn to classify regulatory regions based on sequence features - k-mers. To do this, we borrowed two approaches from the field of natural language processing: (1) ``bag-of-words'' which is commonly used for differentially weighting key words in tasks like sentiment analyses, and (2) a vector-space model using word2vec (vector-k-mers), that captures semantic and linguistic relationships between words. We built ``bag-of-k-mers'' and ``vector-k-mers'' models that distinguish between regulatory and nonregulatory regions with an accuracy above 90\%. Our ``bag-of-k-mers'' achieved higher overall accuracy, while the ``vector-k-mers'' models were more useful in highlighting key groups of sequences within the regulatory regions. These models now provide powerful tools to annotate regulatory regions in other maize lines beyond the reference, at low cost and with high accuracy.},
  langid = {english},
  keywords = {pangenomics},
  file = {D\:\\Works\\zotfile\\Plant Biology2017\\2017_Mejía-Guerra_ik-i_-mer_grammar_uncovers_maize_regulatory_architecture.pdf}
}

