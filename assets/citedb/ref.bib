  file = {D\:\\Works\\zotero\\storage\\32AHQE5H\\Sanderson 等。 - 2021 - A PostgreSQL Tripal solution for large-scale genot.pdf;D\:\\Works\\zotfile\\2021\\2021_Sanderson_A_PostgreSQL_Tripal_solution_for_large-scale_genotypic_and_phenotypic_data.pdf}
