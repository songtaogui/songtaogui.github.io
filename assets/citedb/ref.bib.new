@article{walesAncientDNAReveals2019,
	title = {Ancient {\textless}span style="font-variant:small-caps;"{\textgreater}{DNA}{\textless}/span{\textgreater} reveals the timing and persistence of organellar genetic bottlenecks over 3,000 years of sunflower domestication and improvement},
	volume = {12},
	issn = {1752-4571, 1752-4571},
	shorttitle = {Ancient {\textless}span style="font-variant},
	url = {https://onlinelibrary.wiley.com/doi/10.1111/eva.12594},
	doi = {10.1111/eva.12594},
	abstract = {Here, we report a comprehensive paleogenomic study of archaeological and ethnographic sunflower remains that provides significant new insights into the process of domestication of this important crop. DNA from both ancient and historic contexts yielded high proportions of endogenous DNA, and although archaeological DNA was found to be highly degraded, it still provided sufficient coverage to analyze genetic changes over time. Shotgun sequencing data from specimens from the Eden’s Bluff archaeological site in Arkansas yielded organellar DNA sequence from specimens up to 3,100 years old. Their sequences match those of modern cultivated sunflowers and are consistent with an early domestication bottleneck in this species. Our findings also suggest that recent breeding of sunflowers has led to a loss of genetic diversity that was present only a century ago in Native American landraces. These breeding episodes also left a profound signature on the mitochondrial and plastid haplotypes in cultivars, as two types were intentionally introduced from other Helianthus species for crop improvement. These findings gained from ancient and historic sunflower specimens underscore how future in-­depth gene-­based analyses can advance our understanding of the pace and targets of selection during the domestication of sunflower and other crop species.},
	language = {en},
	number = {1},
	urldate = {2022-05-22},
	journal = {Evolutionary Applications},
	author = {Wales, Nathan and Akman, Melis and Watson, Ray H. B. and Sánchez Barreiro, Fátima and Smith, Bruce D. and Gremillion, Kristen J. and Gilbert, M. Thomas P. and Blackman, Benjamin K.},
	month = jan,
	year = {2019},
	keywords = {evolution, mito},
	pages = {38--53},
	file = {2019_Evolutionary_Applications_Wales_Ancient_span_style=font-variant.pdf:D\:\\Works\\zotfile\\Evolutionary Applications\\2019_Evolutionary_Applications_Wales_Ancient_span_style=font-variant.pdf:application/pdf},
}

@article{ladoukakisEvolutionInheritanceAnimal2017,
	title = {Evolution and inheritance of animal mitochondrial {DNA}: rules and exceptions},
	volume = {24},
	issn = {2241-5793},
	shorttitle = {Evolution and inheritance of animal mitochondrial {DNA}},
	url = {http://jbiolres.biomedcentral.com/articles/10.1186/s40709-017-0060-4},
	doi = {10.1186/s40709-017-0060-4},
	abstract = {Mitochondrial DNA (mtDNA) has been studied intensely for “its own” merit. Its role for the function of the cell and the organism remains a fertile field, its origin and evolution is an indispensable part of the evolution of life and its interaction with the nuclear DNA is among the most important cases of genome synergism and co-evolution. Also, mtDNA was proven one of the most useful tools in population genetics and molecular phylogenetics. In this article we focus on animal mtDNA and discuss briefly how our views about its structure, function and transmission have changed, how these changes affect the information we have accumulated through its use in the fields of phylogeny and population structure and what are the most important questions that remain open for future research.},
	language = {en},
