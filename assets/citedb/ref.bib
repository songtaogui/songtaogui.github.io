@article{2021-Parada-MicroExonator-GB,
  title = {{{MicroExonator}} Enables Systematic Discovery and Quantification of Microexons across Mouse Embryonic Development},
  author = {Parada, Guillermo E. and Munita, Roberto and {Georgakopoulos-Soares}, Ilias and Fernandes, Hugo J. R. and Kedlian, Veronika R. and Metzakopian, Emmanouil and Andres, Maria Estela and Miska, Eric A. and Hemberg, Martin},
  year = {2021},
  month = dec,
  journal = {Genome Biology},
  volume = {22},
  number = {1},
