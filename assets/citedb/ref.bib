@article{2020-Monnahan-Using-BG,
  title = {Using Multiple Reference Genomes to Identify and Resolve Annotation Inconsistencies},
  author = {Monnahan, Patrick J. and Michno, Jean-Michel and O'Connor, Christine and Brohammer, Alex B. and Springer, Nathan M. and McGaugh, Suzanne E. and Hirsch, Candice N.},
