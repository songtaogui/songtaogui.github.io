  author = {Leal, Thiago Peixoto and Furlan, Vinicius C and Gouveia, Mateus Henrique and Saraiva Duarte, Julia Maria and Fonseca, Pablo AS and Tou, Rafael and Scliar, Marilia de Oliveira and de Araujo, Gilderlanio Santana and Costa, Lucas F. and Zolini, Camila and Peixoto, Maria Gabriela Campolina Diniz and Carvalho, Maria Raquel Santos and {Lima-Costa}, Maria Fernanda and Gilman, Robert H and {Tarazona-Santos}, Eduardo and Rodrigues, Ma{\'i}ra Ribeiro},
