@article{2019-Qi-Photoinduced-ACS,
  title = {{Photo-induced Electron Transfer between 4-Thiouracil and Tryptophan}},
  author = {Qi, Qige and Yang, Chunfan and Xia, Ye and Liu, Kunhui and Su, Hongmei},
  year = {2019},
  journal = {Acta Chimica Sinica},
  volume = {77},
