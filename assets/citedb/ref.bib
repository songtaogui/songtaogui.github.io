@article{2021-Orlando-Ancient-NRMP,
  title = {Ancient {{DNA}} Analysis},
  author = {Orlando, Ludovic and Allaby, Robin and Skoglund, Pontus and Der Sarkissian, Clio and Stockhammer, Philipp W. and {\'A}vila-Arcos, Mar{\'i}a C. and Fu, Qiaomei and Krause, Johannes and Willerslev, Eske and Stone, Anne C. and Warinner, Christina},
