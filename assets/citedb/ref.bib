@article{2014-Liu-Critical-PMBR,
  title = {Critical Cis-{{Acting Elements}} and {{Interacting Transcription Factors}}: {{Key Players Associated}} with {{Abiotic Stress Responses}} in {{Plants}}},
  shorttitle = {Critical Cis-{{Acting Elements}} and {{Interacting Transcription Factors}}},
  author = {Liu, Ji-Hong and Peng, Ting and Dai, Wenshan},
