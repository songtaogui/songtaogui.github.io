@article{kouzinePermanganateS1Nuclease2017,
	title = {Permanganate/{S1} {Nuclease} {Footprinting} {Reveals} {Non}-{B} {DNA} {Structures} with {Regulatory} {Potential} across a {Mammalian} {Genome}},
