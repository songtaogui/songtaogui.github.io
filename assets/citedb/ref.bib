@article{2016-Wall-Detecting-GG,
  title = {Detecting {{Recombination Hotspots}} from {{Patterns}} of {{Linkage Disequilibrium}}},
  author = {Wall, Jeffrey D and Stevison, Laurie S},
  year = {2016},
  month = aug,
  journal = {G3 Genes|Genomes|Genetics},
