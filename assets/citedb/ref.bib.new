@article{xiaBidirectionalSelectionUpland2019,
	title = {Bi-directional {Selection} in {Upland} {Rice} {Leads} to {Its} {Adaptive} {Differentiation} from {Lowland} {Rice} in {Drought} {Resistance} and {Productivity}},
	volume = {12},
	issn = {16742052},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1674205218303782},
	doi = {10.1016/j.molp.2018.12.011},
	abstract = {Drought resistance is required in rice breeding to address the challenge of frequent droughts. However, the evolutionary mechanism of rice drought resistance is not fully understood. We investigated the genetic differentiation between upland and lowland rice domesticated in agro-ecosystems with contrasting water–soil conditions using genome-wide SNPs. We estimated morphological differences among upland and lowland rice in drought resistance and productivity through common garden experiments. Upland rice had better drought resistance but poorer productivity. The negative correlations between traits of drought resistance and productivity are attributed to the underlying genetic trade-offs through tight linkages (e.g., DCA1 and OsCesA7) or pleiotropic effects (e.g., LAX1). The genetic trade-offs are common and greatly shape the evolution of drought resistance in upland rice. In genomic regions associated with both productivity and drought resistance, signs of balancing selection were detected in upland rice, while signs of directional selection were detected in lowland rice, potentially contributing to their adaptive differentiation. Signs of balancing selection in upland rice resulted from bi-directional selection during its domestication in drought-prone upland agro-ecosystems. Using genome-wide association analysis, we identiﬁed several valuable quantitative trait loci associated with drought resistance, for which highly differentiated genes should be considered candidates. Bi-directional selection breaking tight linkages by accumulating recombination events would be applicable in breeding water-saving and droughtresistance rice.},
	language = {en},
	number = {2},
	urldate = {2022-05-22},
	journal = {Molecular Plant},
	author = {Xia, Hui and Luo, Zhi and Xiong, Jie and Ma, Xiaosong and Lou, Qiaojun and Wei, Haibin and Qiu, Jie and Yang, Hua and Liu, Guolan and Fan, Longjiang and Chen, Liang and Luo, Lijun},
	month = feb,
	year = {2019},
	keywords = {evolution},
	pages = {170--184},
	file = {2019_Molecular_Plant_Xia_Bi-directional_Selection_in_Upland_Rice_Leads_to_Its_Adaptive_Differentiation.pdf:D\:\\Works\\zotfile\\Molecular Plant\\2019_Molecular_Plant_Xia_Bi-directional_Selection_in_Upland_Rice_Leads_to_Its_Adaptive_Differentiation.pdf:application/pdf},
