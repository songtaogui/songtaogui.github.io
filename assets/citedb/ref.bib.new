	keywords = {systembiology},
	pages = {376},
	file = {2020_Nature_Communications_Burgess_A_robust_and_efficient_method_for_Mendelian_randomization_with_hundreds_of.pdf:D\:\\Works\\zotfile\\Nature Communications\\2020_Nature_Communications_Burgess_A_robust_and_efficient_method_for_Mendelian_randomization_with_hundreds_of.pdf:application/pdf},
}

