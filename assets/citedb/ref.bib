@article{2019-Mousavi-Profiling-NAR,
  title = {Profiling the Genome-Wide Landscape of Tandem Repeat Expansions},
  author = {Mousavi, Nima and {Shleizer-Burko}, Sharona and Yanicky, Richard and Gymrek, Melissa},
  year = {2019},
