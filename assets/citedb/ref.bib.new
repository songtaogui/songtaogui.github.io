@article{dasGenotypeImputationLarge2018,
	title = {Genotype {Imputation} from {Large} {Reference} {Panels}},
	volume = {19},
	issn = {1527-8204, 1545-293X},
	url = {https://www.annualreviews.org/doi/10.1146/annurev-genom-083117-021602},
	doi = {10.1146/annurev-genom-083117-021602},
	abstract = {Genotype imputation has become a standard tool in genome-wide association studies because it enables researchers to inexpensively approximate whole-genome sequence data from genome-wide single-nucleotide polymorphism array data. Genotype imputation increases statistical power, facilitates ﬁne mapping of causal variants, and plays a key role in meta-analyses of genome-wide association studies. Only variants that were previously observed in a reference panel of sequenced individuals can be imputed. However, the rapid increase in the number of deeply sequenced individuals will soon make it possible to assemble enormous reference panels that greatly increase the number of imputable variants. In this review, we present an overview of genotype imputation and describe the computational techniques that make it possible to impute genotypes from reference panels with millions of individuals.},
