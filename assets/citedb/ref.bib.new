	keywords = {str},
	pages = {2453--2465},
	file = {2002_Molecular_Ecology_Li_Microsatellites.pdf:D\:\\Works\\zotfile\\Molecular Ecology\\2002_Molecular_Ecology_Li_Microsatellites.pdf:application/pdf},
}

