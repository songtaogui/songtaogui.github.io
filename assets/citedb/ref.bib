@article{2020-Wang-FINET-BRN,
  title = {{{FINET}}: {{Fast Inferring NETwork}}},
  shorttitle = {{{FINET}}},
  author = {Wang, Anyou and Hai, Rong},
  year = {2020},
  month = dec,
  journal = {BMC Research Notes},
  volume = {13},
