@article{2021-Bhattacharya-MOSTWAS-PG,
  title = {{{MOSTWAS}}: {{Multi-Omic Strategies}} for {{Transcriptome-Wide Association Studies}}},
  shorttitle = {{{MOSTWAS}}},
  author = {Bhattacharya, Arjun and Li, Yun and Love, Michael I.},
