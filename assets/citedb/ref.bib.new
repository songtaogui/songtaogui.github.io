@article{jieSulfurcenteredHemibondRadicals2019,
	title = {Sulfur-centered hemi-bond radicals as active intermediates in {S}-{DNA} phosphorothioate oxidation},
	issn = {0305-1048, 1362-4962},
	url = {https://academic.oup.com/nar/advance-article/doi/10.1093/nar/gkz987/5625538},
	doi = {10.1093/nar/gkz987},
	abstract = {Phosphorothioate (PS) modiﬁcations naturally appear in bacteria and archaea genome and are widely used as antisense strategy in gene therapy. But the chemical effects of PS introduction as a redox active site into DNA (S-DNA) is still poorly understood. Herein, we perform time-resolved spectroscopy to examine the underlying mechanisms and dynamics of the PS oxidation by potent radicals in free model, in dinucleotide, and in S-oligomer. The crucial sulphur-centered hemi-bonded intermediates P–S∴S–P- were observed and found to play critical roles leading to the stable adducts of -P–S–S–P-, which are backbone DNA lesion products. Moreover, the oxidation of the PS moiety in dinucleotides d[GPSG], d[APSA], d[GPSA], d[APSG] and in S-oligomers was monitored in real-time, showing that PS oxidation can compete with adenine but not with guanine. Signiﬁcantly, hole transfer process from A+• to PS and concomitant -P–S∴S–P- formation was observed, demonstrating the base-to-backbone hole transfer unique to S-DNA, which is different from the normally adopted backbone-to-base hole transfer in native DNA. These ﬁndings reveal the distinct backbone lesion pathway brought by the PS modiﬁcation and also imply an alternative -P–S∴S–P-/-P–S–S–P- pathway accounting for the interesting protective role of PS as an oxidation sacriﬁce in bacterial genome.},
	language = {en},
	urldate = {2022-06-22},
	journal = {Nucleic Acids Research},
	author = {Jie, Jialong and Xia, Ye and Huang, Chun-Hua and Zhao, Hongmei and Yang, Chunfan and Liu, Kunhui and Song, Di and Zhu, Ben-Zhan and Su, Hongmei},
	month = nov,
	year = {2019},
	pages = {gkz987},
	file = {2019_Nucleic_Acids_Research_Jie_Sulfur-centered_hemi-bond_radicals_as_active_intermediates_in_S-DNA.pdf:D\:\\Works\\zotfile\\Nucleic Acids Research\\2019_Nucleic_Acids_Research_Jie_Sulfur-centered_hemi-bond_radicals_as_active_intermediates_in_S-DNA.pdf:application/pdf},
}

