  author = {Gui, Songtao and Peng, Jing and Wang, Xiaolei and Wu, Zhihua and Cao, Rui and Salse, J{\'e}r{\^o}me and Zhang, Hongyuan and Zhu, Zhixuan and Xia, Qiuju and Quan, Zhiwu and Shu, Liping and Ke, Wedong and Ding, Yi},
