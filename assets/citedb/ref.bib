@article{2019-Cao-Accuracy-JRSI,
  title = {Accuracy of Parameter Estimation for Auto-Regulatory Transcriptional Feedback Loops from Noisy Data},
  author = {Cao, Zhixing and Grima, Ramon},
  year = {2019},
