@article{2018-Zong-Maize-JoIA,
  title = {Maize {{ABP2}} Enhances Tolerance to Drought and Salt Stress in Transgenic {{Arabidopsis}}},
  author = {Zong, Na and Li, Xing-juan and Wang, Lei and Wang, Ying and Wen, Hong-tao and Li, Ling and Zhang, Xia and Fan, Yun-liu and Zhao, Jun},
