	institution = {Bioinformatics},
	author = {Sirén, Jouni and Monlong, Jean and Chang, Xian and Novak, Adam M. and Eizenga, Jordan M. and Markello, Charles and Sibbesen, Jonas A. and Hickey, Glenn and Chang, Pi-Chuan and Carroll, Andrew and Gupta, Namrata and Gabriel, Stacey and Blackwell, Thomas W. and Ratan, Aakrosh and Taylor, Kent D. and Rich, Stephen S. and Rotter, Jerome I. and Haussler, David and Garrison, Erik and Paten, Benedict},
	month = dec,
