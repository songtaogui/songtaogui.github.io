@article{2021-Xiao-Genetic-GB,
  title = {The Genetic Mechanism of Heterosis Utilization in Maize Improvement},
  author = {Xiao, Yingjie and Jiang, Shuqin and Cheng, Qian and Wang, Xiaqing and Yan, Jun and Zhang, Ruyang and Qiao, Feng and Ma, Chuang and Luo, Jingyun and Li, Wenqiang and Liu, Haijun and Yang, Wenyu and Song, Wenhao and Meng, Yijiang and Warburton, Marilyn L. and Zhao, Jiuran and Wang, Xiangfeng and Yan, Jianbing},
