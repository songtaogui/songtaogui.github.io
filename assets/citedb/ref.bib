  author = {{Van de Weyer}, Anna-Lena and Monteiro, Freddy and Furzer, Oliver J. and Nishimura, Marc T. and Cevik, Volkan and Witek, Kamil and Jones, Jonathan D.G. and Dangl, Jeffery L. and Weigel, Detlef and Bemm, Felix},
  year = {2019},
  month = aug,
  journal = {Cell},
  volume = {178},
  number = {5},
  pages = {1260-1272.e14},
  issn = {00928674},
  doi = {10.1016/j.cell.2019.07.038},
  abstract = {Infectious disease is both a major force of selection in nature and a prime cause of yield loss in agriculture. In plants, disease resistance is often conferred by nucleotide-binding leucine-rich repeat (NLR) proteins, intracellular immune receptors that recognize pathogen proteins and their effects on the host. Consistent with extensive balancing and positive selection, NLRs are encoded by one of the most variable gene families in plants, but the true extent of intraspecific NLR diversity has been unclear. Here, we define a nearly complete species-wide panNLRome in Arabidopsis thaliana based on sequence enrichment and long-read sequencing. The panNLRome largely saturates with approximately 40 well-chosen wild strains, with half of the panNLRome being present in most accessions. We chart NLR architectural diversity, identify new architectures, and quantify selective forces that act on specific NLRs and NLR domains. Our study provides a blueprint for defining pan-NLRomes.},
  langid = {english},
  file = {D\:\\Works\\zotero\\storage\\KLVK9ZS3\\Van de Weyer 等。 - 2019 - A Species-Wide Inventory of NLR Genes and Alleles .pdf;D\:\\Works\\zotfile\\Cell2019\\2019_Cell_Van_de_Weyer_A_Species-Wide_Inventory_of_NLR_Genes_and_Alleles_in_Arabidopsis_thaliana.pdf}
}

