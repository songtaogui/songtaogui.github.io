@article{2018-Mittal-Codon-PNASU,
  title = {Codon Usage Influences Fitness through {{RNA}} Toxicity},
  author = {Mittal, Pragya and Brindle, James and Stephen, Julie and Plotkin, Joshua B. and Kudla, Grzegorz},
