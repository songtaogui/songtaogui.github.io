	keywords = {julia, bioinfo},
	pages = {1935--1945},
	file = {2022_Computational_and_Structural_Biotechnology_Journal_Wang_Noncoding_RNAs_endogenously_rule_the_cancerous_regulatory_realm_while_proteins.pdf:D\:\\Works\\zotfile\\Computational and Structural Biotechnology Journal\\2022_Computational_and_Structural_Biotechnology_Journal_Wang_Noncoding_RNAs_endogenously_rule_the_cancerous_regulatory_realm_while_proteins.pdf:application/pdf},
}

