@article{mingGenomeWideIdentification2020,
	title = {Genome {Wide} {Identification} of {C2H2}-{Type} {Zinc} {Finger} {Proteins} of {Tomato} and {Expression} {Analysis} {Under} {Different} {Abiotic} {Stresses}},
