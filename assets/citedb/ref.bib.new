	keywords = {pangenomics},
	pages = {55},
	file = {2015_Biology_Direct_Caputo_Pan-genomic_analysis_to_redefine_species_and_subspecies_based_on_quantum.pdf:D\:\\Works\\zotfile\\Biology Direct\\2015_Biology_Direct_Caputo_Pan-genomic_analysis_to_redefine_species_and_subspecies_based_on_quantum.pdf:application/pdf},
