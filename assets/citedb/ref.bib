@article{2012-Feldman-Constraint-PNASU,
  title = {Constraint Shapes Convergence in Tetrodotoxin-Resistant Sodium Channels of Snakes},
  author = {Feldman, Chris R. and Brodie, Edmund D. and Brodie, Edmund D. and Pfrender, Michael E.},
  year = {2012},
