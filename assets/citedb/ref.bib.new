@article{hemaniEvaluatingPotentialRole2018,
	title = {Evaluating the potential role of pleiotropy in {Mendelian} randomization studies},
	volume = {27},
	issn = {0964-6906, 1460-2083},
	url = {https://academic.oup.com/hmg/article/27/R2/R195/4996734},
	doi = {10.1093/hmg/ddy163},
	abstract = {Pleiotropy, the phenomenon of a single genetic variant inﬂuencing multiple traits, is likely widespread in the human genome. If pleiotropy arises because the single nucleotide polymorphism (SNP) inﬂuences one trait, which in turn inﬂuences another (‘vertical pleiotropy’), then Mendelian randomization (MR) can be used to estimate the causal inﬂuence between the traits. Of prime focus among the many limitations to MR is the unprovable assumption that apparent pleiotropic associations are mediated by the exposure (i.e. reﬂect vertical pleiotropy), and do not arise due to SNPs inﬂuencing the two traits through independent pathways (‘horizontal pleiotropy’). The burgeoning treasure trove of genetic associations yielded through genome wide association studies makes for a tantalizing prospect of phenome-wide causal inference. Recent years have seen substantial attention devoted to the problem of horizontal pleiotropy, and in this review we outline how newly developed methods can be used together to improve the reliability of MR.},
	language = {en},
	number = {R2},
	urldate = {2022-05-22},
	journal = {Human Molecular Genetics},
	author = {Hemani, Gibran and Bowden, Jack and Davey Smith, George},
	month = aug,
	year = {2018},
	keywords = {systembiology},
	pages = {R195--R208},
	file = {2018_Human_Molecular_Genetics_Hemani_Evaluating_the_potential_role_of_pleiotropy_in_Mendelian_randomization_studies.pdf:D\:\\Works\\zotfile\\Human Molecular Genetics\\2018_Human_Molecular_Genetics_Hemani_Evaluating_the_potential_role_of_pleiotropy_in_Mendelian_randomization_studies.pdf:application/pdf},
}

